{
  "name": "urlint-cli",
  "description": "Check the availability of your links.",
  "homepage": "https://documentup.com/urlint/urlint-cli",
  "version": "1.0.4",
  "main": "index.js",
  "bin": {
    "urlint": "./bin/cli/index.js"
  },
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urlint/urlint-cli.git"
  },
  "bugs": {
    "url": "https://github.com/urlint/urlint-cli/issues"
  },
  "keywords": [
    "classify",
    "cli",
    "http",
    "link",
    "links",
    "lint",
    "linter",
    "status",
    "url",
    "validate"
  ],
  "dependencies": {
    "@metascraper/helpers": "~3.9.2",
    "chalk": "~2.4.0",
    "got": "~8.3.0",
    "html-urls": "~1.0.3",
    "is-ci": "~1.1.0",
    "lodash": "~4.17.4",
    "meow": "~4.0.0",
    "neat-log": "~2.1.1",
    "normalize-url": "~2.0.1",
    "ora": "~2.0.0",
    "update-notifier": "~2.5.0",
    "urlint": "~1.0.4",
    "xml-urls": "~1.0.1"
  },
  "devDependencies": {
    "ava": "latest",
    "coveralls": "latest",
    "finepack": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "nyc": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard-markdown && standard",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js {core,test,bin}/**/*.js --single-quote",
    "test": "nyc ava"
  },
  "preferGlobal": true,
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ]
  }
}
